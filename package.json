{
  "name": "agenda-nest",
  "version": "2.1.0",
  "description": "A lightweight job scheduler for NestJS",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublish": "npm run build",
    "prepare": "npm run build && husky install",
    "build": "rimraf -rf dist && tsc -p tsconfig.json",
    "lint": "eslint 'lib/**/*.ts'",
    "test": "npm run lint && npm run test:e2e",
    "test:e2e": "jest --config=e2e/jest-e2e.config.ts --detectOpenHandles --runInBand --forceExit",
    "release": "npx standard-version"
  },
  "author": "Jonathan Golden <jongolden17@gmail.com>",
  "license": "MIT",
  "url": "https://github.com/jongolden/agenda-nest#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/jongolden/agenda-nest"
  },
  "keywords": [
    "job",
    "jobs",
    "cron",
    "scheduler",
    "runner",
    "agenda",
    "nest",
    "nestjs"
  ],
  "peerDependencies": {
    "@nestjs/common": "8.x || 9.x || 10.x || 11.x",
    "@nestjs/core": "8.x || 9.x || 10.x || 11.x"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.3.0",
    "@commitlint/config-angular": "^20.3.0",
    "@commitlint/config-conventional": "^20.3.0",
    "@jest/types": "^30.2.0",
    "@nestjs/common": "^11.1.11",
    "@nestjs/core": "^11.1.11",
    "@nestjs/testing": "^11.1.11",
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "mongodb-memory-server": "^11.0.1",
    "prettier": "^3.7.4",
    "rimraf": "^6.1.2",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.51.0"
  },
  "dependencies": {
    "agenda-ts": "^1.7.1"
  }
}
